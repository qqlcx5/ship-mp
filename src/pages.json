{
  "globalStyle": {
    "navigationStyle": "custom",
    "navigationBarTitleText": "海洋智控",
    "navigationBarBackgroundColor": "#0B1426",
    "navigationBarTextStyle": "white",
    "backgroundColor": "#0B1426"
  },
  "easycom": {
    "autoscan": true,
    "custom": {
      "^fg-(.*)": "@/components/fg-$1/fg-$1.vue",
      "^wd-(.*)": "wot-design-uni/components/wd-$1/wd-$1.vue",
      "^(?!z-paging-refresh|z-paging-load-more)z-paging(.*)": "z-paging/components/z-paging$1/z-paging$1.vue"
    }
  },

  "pages": [
    {
      "path": "pages/login/login",
      "type": "home",
      "style": {
        "navigationStyle": "custom",
        "navigationBarTitleText": "海洋智控登录"
      }
    },
    {
      "path": "pages/dashboard/dashboard",
      "type": "page",
      "style": {
        "navigationStyle": "custom",
        "navigationBarTitleText": "主控台"
      }
    },
    {
      "path": "pages/manual/manual",
      "type": "page",
      "style": {
        "navigationStyle": "custom",
        "navigationBarTitleText": "手动导航"
      }
    },
    {
      "path": "pages/cruise/cruise",
      "type": "page",
      "style": {
        "navigationStyle": "custom",
        "navigationBarTitleText": "自动巡航"
      }
    },
    {
      "path": "pages/ai/ai",
      "type": "page",
      "style": {
        "navigationStyle": "custom",
        "navigationBarTitleText": "AI管理"
      }
    },
    {
      "path": "pages/management/management",
      "type": "page",
      "style": {
        "navigationStyle": "custom",
        "navigationBarTitleText": "综合管理"
      }
    },
    {
      "path": "pages/index/index",
      "type": "page",
      "style": {
        "navigationStyle": "custom",
        "navigationBarTitleText": "首页"
      }
    },
    {
      "path": "pages/about/about",
      "type": "page",
      "style": {
        "navigationBarTitleText": "关于"
      }
    },
    {
      "path": "pages/about/alova",
      "type": "page",
      "style": {
        "navigationBarTitleText": "Alova 请求演示"
      }
    },
    {
      "path": "pages/about/vue-query",
      "type": "page",
      "style": {
        "navigationBarTitleText": "Vue Query 请求演示"
      }
    }
  ],
  "subPackages": [
    {
      "root": "pages-sub",
      "pages": [
        {
          "path": "demo/index",
          "type": "page",
          "layout": "default",
          "style": {
            "navigationBarTitleText": "分包页面"
          }
        }
      ]
    }
  ]
}
